{
	"name": "narsil/settings",
	"description": "Narsil Settings",
	"license": "MIT",
	"keywords": [
		"inertia",
		"laravel",
		"narsil",
		"php",
		"react",
		"settings"
	],
	"authors": [
		{
			"name": "Jonathan Rigaux"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-forms.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-localization.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-menus.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-policies.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-storage.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-tree.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Narsileon/narsil-ui.git"
		}
	],
	"require": {
		"php": "^8.2",
		"inertiajs/inertia-laravel": "^1.0",
		"laravel/fortify": "^1.21",
		"laravel/framework": "^11.3",
		"mobiledetect/mobiledetectlib": "^4.8",
		"narsil/forms": "dev-main",
		"narsil/localization": "dev-main",
		"narsil/menus": "dev-main",
		"narsil/policies": "dev-main"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Narsil\\Settings\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Narsil\\Settings\\NarsilSettingsServiceProvider"
			]
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	}
}
